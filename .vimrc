" Backend Settings
set nocompatible
filetype on
filetype plugin on
filetype indent on 

" View settings
syntax on
set number relativenumber
set cursorline
set cursorcolumn
set incsearch
set smartcase
set showmode
set showcmd
set showmatch
set hlsearch
set history=1000
set noerrorbells
set ignorecase
set history=1000
set t_Co=256
set laststatus-2
set wildmenu
set autoread
set clipboard=unnamed

" Wildmenu
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx

" Formatting things
set shiftwidth=4
set tabstop=4
set softtabstop=4
set expandtab
set smarttab
set smartindent
set autoindent

colorscheme elflord


" Key Binds



" Macros
let @c = "0i#j"
