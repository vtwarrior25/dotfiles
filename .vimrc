" Backend Settings
set nocompatible
filetype on
filetype plugin on
filetype indent on 

" View settings
syntax on
set number
set cursorline
set cursorcolumn
set incsearch
set smartcase
set showmode
set showcmd
set showmatch
set hlsearch
set history=1000
set noerrorbells

" Wildmenu
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx

" Formatting things
set shiftwidth=4
set tabstop=4
set softtabstop=4
set expandtab
set smartindent
set autoindent

colorscheme elflord


" Key Binds



" Macros
let @c = "0i#j"
